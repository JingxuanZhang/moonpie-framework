{
    "name": "moonpie/framework",
    "description": "ThinkPHP系统增强版",
    "type": "project",
    "keywords": [
        "framework",
        "thinkphp",
        "thinkphp in action"
    ],
    "authors": [
        {
            "name": "johnzhang",
            "email": "875010341@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-dom": "*",
        "ext-ctype": "*",
        "topthink/framework": "5.0.24",
        "topthink/think-migration": "1.*",
        "moonpie/macro": "dev-master",
        "symfony/lock": "~3.4",
        "league/flysystem": "~1.0",
        "wikimedia/composer-merge-plugin": "dev-master",
        "zendframework/zend-permissions-acl": "^2.7",
        "drupal/core-graph": "^8.7"
    },
    "autoload": {
        "psr-4": {
            "app\\": "application"
        }
    },
    "extra": {
        "think-path": "thinkphp",
        "merge-plugin": {
            "include": [
                "application/*/composer.json",
                "plugin/**/*/composer.json",
                "plugin/*/composer.json"
            ],
            "merge-dev": true
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-autoload-dump": [
            "app\\common\\script\\SystemInitScript::onInstall"
        ]
    }
}
