<manifest xmlns="http://moonpie-framework.localhost.com/dic/plugin">
    <title>系统统一后台</title>
    <version>1.0.0</version>
    <code>manager</code>
    <description>简单实现一个基于AmazeUI的后台</description>
    <category>后台管理</category>
    <commands>
        <command>app\manager\command\AdminPrepareCommand</command>
    </commands>
    <routes>
        <route name="main" target="route.php"/>
    </routes>
    <providers>
        <provider name="basic_provider" target="app\manager\service\provider\ManagerProvider"/>
    </providers>
</manifest>